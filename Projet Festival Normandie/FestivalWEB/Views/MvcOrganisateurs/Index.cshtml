@model IEnumerable<FestivalAPI.Models.Organisateur>

@{
    ViewData["Title"] = "Index";
}
<p><br /></p>
<p><br /></p>
<p><br /></p>
<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prenom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mot_de_passe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Festival)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Nom)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Prenom)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Mot_de_passe)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Festival.Nom_Festival)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.OrganisateurID">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.OrganisateurID">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.OrganisateurID">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
<p>
    <a asp-action="Create">Create New</a>
</p>
