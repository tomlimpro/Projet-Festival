@model IEnumerable<FestivalAPI.Models.Hebergement>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TypeHebergement)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LienHebergement)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Festival)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TypeHebergement)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LienHebergement)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Festival.Nom_Festival)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.HebergementID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.HebergementID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.HebergementID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
